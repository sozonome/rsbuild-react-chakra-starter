{
  "name": "rsbuild-react-chakra-starter",
  "version": "0.1.0",
  "author": "sozonome",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource-variable/plus-jakarta-sans": "^5.1.0",
    "framer-motion": "^11.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2"
  },
  "scripts": {
    "dev": "rsbuild dev",
    "build": "rsbuild build",
    "build-and-analyze": "cross-env RSDOCTOR=true pnpm build",
    "preview": "rsbuild preview",
    "biome:check": "biome check",
    "biome:fix": "pnpm biome:check --write",
    "type:check": "tsc --noEmit",
    "turbo": "pnpm dlx turbo@2.0.9",
    "check:turbo": "pnpm turbo biome:check type:check",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 commit-and-tag-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky",
    "knip": "knip"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.1",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@rsbuild/core": "1.0.4",
    "@rsbuild/plugin-assets-retry": "1.0.1",
    "@rsbuild/plugin-image-compress": "^1.0.2",
    "@rsbuild/plugin-react": "1.0.2",
    "@rsbuild/plugin-svgr": "1.0.2",
    "@rsbuild/plugin-type-check": "1.0.1",
    "@rsdoctor/rspack-plugin": "^0.4.2",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "commit-and-tag-version": "^12.4.4",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "husky": "^9.1.6",
    "knip": "^5.30.2",
    "lint-staged": "^15.2.10",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20.14.x",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.1.1"
}
